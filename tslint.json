{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "array-type": false,
        "object-literal-sort-keys": false,
        "arrow-parens": false,
        "no-bitwise": false,
        "max-classes-per-file": false,
        "no-consecutive-blank-lines": false,
        "unified-signatures": false
    },
    "rulesDirectory": []
}